{
  "dashboard": {
    "id": null,
    "title": "Sensor Fuzzing Framework Metrics",
    "tags": ["sensor-fuzz", "monitoring"],
    "timezone": "browser",
    "panels": [
      {
        "id": 1,
        "title": "System CPU Usage",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(sensor_fuzz_system_cpu_seconds_total[5m])",
            "legendFormat": "CPU Usage"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 0
        }
      },
      {
        "id": 2,
        "title": "System Memory Usage",
        "type": "graph",
        "targets": [
          {
            "expr": "sensor_fuzz_system_memory_bytes / 1024 / 1024",
            "legendFormat": "Memory (MB)"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 0
        }
      },
      {
        "id": 3,
        "title": "Fuzzing Operations",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(sensor_fuzz_operations_total[5m])",
            "legendFormat": "Operations/sec"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 8
        }
      },
      {
        "id": 4,
        "title": "AI Training Progress",
        "type": "graph",
        "targets": [
          {
            "expr": "sensor_fuzz_ai_training_epochs_total",
            "legendFormat": "Training Epochs"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 8
        }
      }
    ],
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "timepicker": {},
    "templating": {
      "list": []
    },
    "annotations": {
      "list": []
    },
    "refresh": "5s",
